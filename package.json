{
    "name": "vyvr",
    "version": "1.0.0",
    "main": "app.js",
    "repository": "https://github.com/amnesiacu/vyvr.git",
    "author": "amnesiacu <jonas.vandevyver@gmail.com>",
    "license": "MIT",
    "scripts": {
        "eslint": "eslint \"./**/*.js\"",
        "lint": "yarn eslint",
        "test": "mocha --compilers js:babel-core/register",
        "build": "rm -rf dist && babel ./ --out-dir dist/ --ignore ./sass,./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
        "serve": "node dist/app.js",
        "dev": "nodemon -w app.js -w views/index.vue -w views/* --exec \"yarn build && node dist/app.js \""
    },
    "dependencies": {
        "express": "^4.15.4",
        "express-vue": "^4.0.6",
        "morgan": "^1.8.2",
        "vue": "^2.4.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "6.18.0",
        "chai": "3.5.0",
        "eslint": "3.10.2",
        "mocha": "3.1.2",
        "nodemon": "1.11.0"
    }
}
